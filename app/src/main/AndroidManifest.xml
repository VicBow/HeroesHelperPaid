<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="motivationalapps.heroeshelperPaid">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="motivationalapps.heroeshelperPaid.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="motivationalapps.heroeshelperPaid.FloatingWidgetService"
            android:enabled="true"
            android:exported="false" />

        <activity android:name="motivationalapps.heroeshelperPaid.Feedback"/>
    </application>

</manifest>